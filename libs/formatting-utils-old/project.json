{
  "name": "formatting-utils-old",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/formatting-utils-old/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/formatting-utils-old",
        "main": "libs/formatting-utils-old/src/index.ts",
        "tsConfig": "libs/formatting-utils-old/tsconfig.lib.json",
        "assets": ["libs/formatting-utils-old/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/formatting-utils-old/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/formatting-utils-old/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
